{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/tasas.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/tasas",
      "dest": "/api/tasas.js"
    },
    {
      "src": "/",
      "dest": "/index.js"
    }
  ],
  "functions" : {
    "api/tasas.js" : {
      "maxDuration": 10
    }
  }
}

{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/tasas.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/tasas",
      "dest": "/api/tasas.js",
      "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"
            ]
    },
    {
      "src": "/",
      "dest": "/index.js",
      "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"
            ]
    }
  ]
}
